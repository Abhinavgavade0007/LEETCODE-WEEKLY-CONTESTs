# FIRST ONE :3688. Bitwise OR of Even Numbers in an Array
class Solution {
public:
    int evenNumberBitwiseORs(vector<int>& nums) {
        int ans=0;
        for(auto it:nums) {
            if(it%2==0) {
                ans=ans|it;
            }
        }
        return ans;
    }
};
# SSECOND ONE : 3689. Maximum Total Subarray Value I
class Solution {
public:
    long long maxTotalValue(vector<int>& nums, int k) {
        long long ans=0;
        int maxi=*max_element(nums.begin(),nums.end());
        int mini=*min_element(nums.begin(),nums.end());
        ans=(long long)(maxi-mini);
        return ans*k;
    }
};
